----------- RENEWAL SALES CHANNEL MAKER CONTAINER -----------
Renewal_Sales_Channel_Maker_inner_join_1 AS (
SELECT
        q1.*,
        q2.SALES_CHANNEL,
        q2.SALES_GROUP,
        q2.REV_CHNL_DESC,
        q2.CHANNEL_MARKER_NEW,
        q2.CHANNEL_MARKER_RNWL
    FROM
        Sales_Type_Rule_New_Renewal_Filter_Inner_Join_Award_Type q1
    INNER JOIN 
    (
    SELECT
      DISTINCT 
        SALES_CHANNEL,
        SALES_GROUP,
        REV_CHNL_DESC,
        TERR_TYPE,
        NEW_SALE_TYPE,
        COALESCE(TRIM(value), 'NULL') AS RNWL_SALE_TYPE,
        CHANNEL_MARKER_NEW,
        CHANNEL_MARKER_RNWL
    FROM MYDATASPACE.A208043_FINANCE_CONSREVENUE_UAT1.SRU_SALES_CMSNV_TC_TERR_TYPE_LKP$,
    LATERAL FLATTEN(input => SPLIT(COALESCE(RNWL_SALE_TYPE, ''), ';'))
    ) q2
ON
    q1."Territory Type Code" = q2.TERR_TYPE AND q1.SALES_TYPE = q2.RNWL_SALE_TYPE
WHERE
    q1.RULE_NEW_RENEWAL_SALE != 'NEW'
),

