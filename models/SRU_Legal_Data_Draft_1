        CLSFITION4_DESC,
        CLSFITION5_DESC,
        ZW_CUSTOMER_NUMBER AS "ZW Customer Number",
        ZW_CUSTOMER_DESC AS "ZW Customer Desc",
        ZW_CITY AS "ZW City",
        ZW_REGION AS "ZW Region",
